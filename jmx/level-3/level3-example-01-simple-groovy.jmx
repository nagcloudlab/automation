          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor - Generate Payment Reference">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// ========================================
// Generate Unique Payment Reference
// ========================================

// Step 1: Get current timestamp
def timestamp = new Date().getTime()
log.info(&quot;Timestamp: &quot; + timestamp)

// Step 2: Generate random number (0 to 99999)
def random = new Random().nextInt(99999)
log.info(&quot;Random number: &quot; + random)

// Step 3: Combine to create payment reference
def paymentRef = &quot;PAY${timestamp}${random}&quot;
log.info(&quot;Generated Payment Reference: &quot; + paymentRef)

// Step 4: Store in JMeter variable
vars.put(&quot;paymentReference&quot;, paymentRef)

// Step 5: Also generate a transaction ID
def txnId = &quot;TXN${timestamp}${random + 1}&quot;
vars.put(&quot;transactionId&quot;, txnId)
log.info(&quot;Generated Transaction ID: &quot; + txnId)

// Step 6: Generate random amount
def amount = 100 + new Random().nextInt(4900) // 100 to 5000
vars.put(&quot;randomAmount&quot;, amount.toString())
log.info(&quot;Generated Amount: &quot; + amount)

log.info(&quot;===== All variables generated successfully! =====&quot;)</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>

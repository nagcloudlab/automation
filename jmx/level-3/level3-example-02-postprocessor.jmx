            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor - Process Payment Response">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">// ========================================
// Process Payment Response
// ========================================

// Step 1: Get the response as text
def response = prev.getResponseDataAsString()
log.info(&quot;Response received, length: &quot; + response.length())

// Step 2: Parse JSON response
import groovy.json.JsonSlurper
def jsonSlurper = new JsonSlurper()

try {
    def jsonResponse = jsonSlurper.parseText(response)
    
    // Step 3: Check if payment was successful
    if (jsonResponse.success) {
        log.info(&quot;✅ Payment SUCCESS&quot;)
        
        // Extract transaction ID
        def txnId = jsonResponse.data.transactionId
        vars.put(&quot;extractedTxnId&quot;, txnId)
        log.info(&quot;Transaction ID: &quot; + txnId)
        
        // Extract amount and charges
        def amount = jsonResponse.data.amount
        def processingFee = jsonResponse.data.charges?.processingFee ?: 0
        def gst = jsonResponse.data.charges?.gst ?: 0
        def totalCharges = jsonResponse.data.charges?.total ?: 0
        
        // Calculate total deducted
        def totalDeducted = amount + totalCharges
        vars.put(&quot;totalDeducted&quot;, totalDeducted.toString())
        log.info(&quot;Amount: ₹&quot; + amount + &quot; + Charges: ₹&quot; + totalCharges + &quot; = Total: ₹&quot; + totalDeducted)
        
        // Extract balance
        if (jsonResponse.data.balanceAfter) {
            def balanceAfter = jsonResponse.data.balanceAfter
            vars.put(&quot;currentBalance&quot;, balanceAfter.toString())
            log.info(&quot;Balance After Transaction: ₹&quot; + balanceAfter)
        }
        
        // Extract beneficiary info
        if (jsonResponse.data.beneficiary) {
            def beneficiaryName = jsonResponse.data.beneficiary.name
            vars.put(&quot;beneficiaryName&quot;, beneficiaryName)
            log.info(&quot;Paid to: &quot; + beneficiaryName)
        }
        
        // Business rule validation
        if (totalDeducted &gt; 5000) {
            log.warn(&quot;⚠️ HIGH VALUE TRANSACTION: ₹&quot; + totalDeducted)
            vars.put(&quot;highValueFlag&quot;, &quot;true&quot;)
        } else {
            vars.put(&quot;highValueFlag&quot;, &quot;false&quot;)
        }
        
        // Store status
        vars.put(&quot;paymentStatus&quot;, &quot;SUCCESS&quot;)
        
    } else {
        log.error(&quot;❌ Payment FAILED: &quot; + jsonResponse.message)
        vars.put(&quot;paymentStatus&quot;, &quot;FAILED&quot;)
        vars.put(&quot;errorMessage&quot;, jsonResponse.message ?: &quot;Unknown error&quot;)
    }
    
} catch (Exception e) {
    log.error(&quot;Error parsing response: &quot; + e.message)
    vars.put(&quot;paymentStatus&quot;, &quot;ERROR&quot;)
    vars.put(&quot;errorMessage&quot;, &quot;Failed to parse response&quot;)
}

log.info(&quot;===== Response processing complete! =====&quot;)</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
